<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="mySQL.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:mySQL"
    Shell.FlyoutBehavior="Disabled">

    <FlyoutItem Title="Astronomy" Icon="moon.png">
        <ShellContent Title="Input Form" Icon="moon.png" Route="InputPage"
            ContentTemplate="{DataTemplate local:InputPage}"/>

        <ShellContent Title="View Players" Icon="sun.png" Route="ViewPage"
            ContentTemplate="{DataTemplate local:ViewPage}"/>
    </FlyoutItem>

</Shell>
